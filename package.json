{
    "name": "comfycat",
    "version": "1.0.0",
    "scripts": {
        "front": "cd app_frontend && ng serve",
        "back": "cd app_services && yarn start:dev",
        "front-package": "cd app_frontend && rm -Rf node_modules/ && yarn",
        "back-package": "cd app_services && rm -Rf node_modules/ && yarn",
        "format": "prettier --write \"app_frontend/src/**/*.ts\" \"app_services/src/**/*.ts\""
    },
    "devDependencies": {
      "prettier": "2.4.1"
    }
}